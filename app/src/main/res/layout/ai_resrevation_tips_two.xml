<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="45dp">

        <LinearLayout
            android:id="@+id/go_back"
            android:layout_width="@dimen/dimen50dp"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/left_img"
                android:layout_width="@dimen/dp_10"
                android:layout_height="@dimen/dp_15"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="18dp"
                android:src="@mipmap/icon_back"
                android:visibility="gone" />
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="提示"
            android:textColor="#333333"
            android:textSize="@dimen/dp_17" />
    </RelativeLayout>

    <View style="@style/Base_line_Horizontal" />

    <TextView
        android:id="@+id/tv_tipsTwoMessage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimen80dp"
        android:layout_marginTop="@dimen/dimen35dp"
        android:layout_marginRight="@dimen/dimen80dp"
        android:layout_marginBottom="@dimen/dimen20dp"
        android:gravity="center"
        android:lineSpacingExtra="@dimen/dimen5dp"
        android:text="您还有3个仓库未进行装卸货， 您是否确定取消"
        android:textColor="@color/black_333"
        android:textSize="@dimen/dimen15dp" />

    <Button
        android:id="@+id/btn_tipsTwo_continue"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen44dp"
        android:layout_marginLeft="@dimen/dimen65dp"
        android:layout_marginTop="@dimen/dimen35dp"
        android:layout_marginRight="@dimen/dimen65dp"
        android:background="@drawable/yt_button_1"
        android:gravity="center"
        android:text="继续预约"
        android:textColor="@color/white"
        android:textSize="@dimen/dimen15dp" />

    <Button
        android:id="@+id/btn_tipsTwo_cancel"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen44dp"
        android:layout_marginLeft="@dimen/dimen65dp"
        android:layout_marginTop="@dimen/dimen8dp"
        android:layout_marginRight="@dimen/dimen65dp"
        android:layout_marginBottom="@dimen/dimen40dp"
        android:background="@drawable/yt_button_3"
        android:gravity="center"
        android:text="确认取消"
        android:textColor="@color/color_theme"
        android:textSize="@dimen/dimen15dp" />
</LinearLayout>