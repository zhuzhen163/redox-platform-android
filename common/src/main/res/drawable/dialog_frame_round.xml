<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- gjx -->
    <!-- 连框颜色值 -->
    <item>
        <shape>
            <solid android:color="#fdfdfd"
            />

            <corners android:radius="10dp" />
        </shape>
    </item>
    <!-- 主体背景颜色值 -->
    <item
    >
        <shape>
            <gradient
                android:angle="90"
                android:centerColor="#fdfdfd"
                android:centerX="0.5"
                android:centerY="0.5"

                android:endColor="#fdfdfd"
                android:startColor="#fdfdfd"
                android:type="linear"/>


            <!-- 应该是圆角 -->
            <corners android:radius="10dp" />
        </shape>
    </item>

</layer-list>